{
  "name": "Wcc",
  "version": "0.1.0",
  "description": "C-compiler for wasm",
  "main": "index.js",
  "scripts": {
    "start": "npm run default",
    "default": "npm run build-assets && gulp",
    "build": "gulp build",
    "build-assets": "make assets",
    "watch": "gulp watch",
    "server": "webpack-dev-server --content-base public",
    "test": "gulp test",
    "lint": "gulp lint",
    "lint:fix": "eslint --fix src/www/**/*.ts",
    "release": "gulp release",
    "clean": "gulp clean"
  },
  "author": "tyfkda",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyfkda/xcc"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/preset-env": "~7.16.4",
    "@babel/register": "~7.16.0",
    "@types/ace": "~0.0.48",
    "@types/jest": "~27.0.3",
    "@types/node": "~16.11.11",
    "@typescript-eslint/eslint-plugin": "~5.5.0",
    "@typescript-eslint/parser": "~5.5.0",
    "browser-sync": "~2.27.7",
    "clone": "~2.1.2",
    "commander": "~8.3.0",
    "del": "~6.0.0",
    "eslint": "~8.4.0",
    "gulp": "~4.0.2",
    "gulp-cssnano": "~2.1.3",
    "gulp-ejs": "~5.1.0",
    "gulp-eslint": "~6.0.0",
    "gulp-htmlmin": "~5.0.1",
    "gulp-jest": "~4.0.4",
    "gulp-plumber": "~1.2.1",
    "gulp-sass": "~5.0.0",
    "gulp-util": "~3.0.8",
    "jest": "~27.4.3",
    "jest-cli": "~27.4.3",
    "sass": "~1.44.0",
    "ts-jest": "~27.0.7",
    "ts-loader": "~9.2.6",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.2",
    "webpack": "~5.64.4",
    "webpack-stream": "~7.0.0"
  },
  "dependencies": {
    "core-js": "~3.19.2"
  }
}
